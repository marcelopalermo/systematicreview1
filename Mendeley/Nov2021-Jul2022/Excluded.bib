   author = {Vishwajeet Dwivedy and Harsh Deep Shukla and Pradeep Kumar Roy},
   doi = {10.1016/J.COMPELECENG.2022.108325},
   journal = {Computers and Electrical Engineering},
   pages = {108325},
   title = {LMNet: Lightweight multi-scale convolutional neural network architecture for COVID-19 detection in IoMT environment},
   url = {https://linkinghub.elsevier.com/retrieve/pii/S0045790622005468},
