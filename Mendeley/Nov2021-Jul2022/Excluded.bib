   title = {Long-COVID diagnosis: From diagnostic to advanced AI-driven models},
   volume = {148},
   url = {https://linkinghub.elsevier.com/retrieve/pii/S0720048X22000146},
   year = {2022},
}
@article{Cavalcanti2022,
   author = {Joao Henrique Cavalcanti and Tibor Kovács and Andrea Kő},
