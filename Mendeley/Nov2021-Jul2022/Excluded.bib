   doi = {10.1007/978-3-030-97546-3_53},
   isbn = {978-3-030-97545-6},
   journal = {AI 2021: Advances in Artificial Intelligence: 34th Australasian Joint Conference, AI 2021, Sydney, NSW, Australia, February 2–4, 2022, Proceedings},
   keywords = {Behaviour understanding,Engagement prediction,Time-series ConvNet},
   pages = {658-667},
   publisher = {Springer-Verlag},
   title = {Video-Based Student Engagement Estimation via Time Convolution Neural Networks for Remote Learning},
   url = {https://doi-org.ez101.periodicos.capes.gov.br/10.1007/978-3-030-97546-3_53},
   year = {2022},
}
