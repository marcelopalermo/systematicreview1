   journal = {Comput. Biol. Med.},
   keywords = {Computerized tomography (CT),Coronavirus disease 2019,Ensemble classifier,Pre-trained models,Severe Acute Respiratory Syndrome CoronaVirus 2 (S,Transfer learning},
