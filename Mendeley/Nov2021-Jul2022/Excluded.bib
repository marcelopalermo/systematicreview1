   journal = {Design, Operation and Evaluation of Mobile Communications: Third International Conference, MOBILE 2022, Held as Part of the 24th HCI International Conference, HCII 2022, Virtual Event, June 26 – July 1, 2022, Proceedings},
   keywords = {Mobile application,Post-pandemic,Smart tourism,Travel agency},
   pages = {60-73},
   publisher = {Springer-Verlag},
   title = {The Effectiveness of Smart Tourism in Malaysia in Covid-19 Post-Pandemic Era: A Case Study},
